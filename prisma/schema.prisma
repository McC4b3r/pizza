datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model toppings {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar
  pizzas pizzas[]
}

model pizzas {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar
  toppings toppings[]
}
